{
  "cli": "nx",
  "id": "api-feature",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "appName": {
      "type": "string",
      "description": "Application name",
      "default": "api"
    }
  },
  "required": ["name", "appName"]
}
