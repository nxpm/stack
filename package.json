{
  "name": "nxpm-stack",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target build --all",
    "test": "nx test",
    "test:ci": "nx run-many --target test --all && nx run-many --target e2e --all",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write --base=remotes/origin/main --head=HEAD",
    "format:write": "nx format:write --base=remotes/origin/main --head=HEAD",
    "format:check": "nx format:check --base=remotes/origin/main --head=HEAD",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@nrwl/nx-cloud": "^10.1.9",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@ngneat/tailwind": "^1.1.0",
    "@nrwl/angular": "10.4.4",
    "@nrwl/cli": "10.4.4",
    "@nrwl/eslint-plugin-nx": "10.4.4",
    "@nrwl/jest": "10.4.4",
    "@nrwl/nest": "10.4.4",
    "@nrwl/nx-plugin": "10.4.4",
    "@nrwl/tao": "10.4.4",
    "@nrwl/workspace": "10.4.4",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "26.0.15",
    "@types/node": "~14.14.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "dotenv": "8.2.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "husky": "^4.3.0",
    "jest": "26.6.3",
    "lint-staged": "^10.5.2",
    "nxpm": "^1.15.0",
    "prettier": "^2.2.0",
    "ts-jest": "26.4.4",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,graphql}": [
      "yarn run format"
    ]
  }
}
